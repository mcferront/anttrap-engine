# based on blender's principled bsdf #https://docs.blender.org/manual/en/latest/render/cycles/nodes/types/shaders/principled.htmlPass=$PASSShader=$SHADERcb_world<float4x4>=$WORLD_MATRIXcb_view_proj<float4x4>=$VP_MATRIXcb_base_color<float4>=$BASE_COLORcb_subsurface_color<float4>=$SUBSURFACE_COLORcb_subsurface_radius<float3>=$SUBSURFACE_RADIUScb_normal<float3>=$NORMALcb_clearcoat_normal<float3>=$CLEARCOAT_NORMALcb_tangent<float3>=$TANGENTcb_subsurface<float>=$SUBSURFACEcb_metallic<float>=$METALLICcb_specular<float>=$SPECULARcb_specular_tint<float>=$SPECULAR_TINTcb_roughness<float>=$ROUGHNESScb_anisotropic<float>=$ANISOTROPICcb_anisotropic_rotation<float>=$ANISOTROPIC_ROTATIONcb_sheen<float>=$SHEENcb_sheen_tint<float>=$SHEEN_TINTcb_clearcoat<float>=$CLEARCOATcb_clearcoat_roughness<float>=$CLEARCOAT_ROUGHNESScb_ior<float>=$IORcb_transmission<float>=$TRANSMISSIONcb_transmission_roughness<float>=$TRANSMISSION_ROUGHNESS#ambient_light<float4>=$LIGHT_AMBIENT#num_lights<float4>=$LIGHT_COUNT#light_dir<float4,16>=$LIGHT_DIR#light_pos<float4,16>=$LIGHT_POS_TYPE#light_color<float4,16>=$LIGHT_COLOR#light_atten<float4,16>=$LIGHT_ATTENDepthTest=TrueDepthFunc=GreaterEqualDepthWrite=TrueAlphaBlend=FalseCullMode=BackSourceBlend=SourceAlpha DestBlend=InvSourceAlpha